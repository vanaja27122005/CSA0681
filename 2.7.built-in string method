def strStr(haystack, needle):
    return haystack.find(needle)
haystack1 = "sadbutsad"
needle1 = "sad"
print("Input:", haystack1, ",", needle1)
print("Output:", strStr(haystack1, needle1))  # Output: 0
haystack2 = "leetcode"
needle2 = "leeto"
print("Input:", haystack2, ",", needle2)
print("Output:", strStr(haystack2, needle2))  # Output: -1
